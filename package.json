		"prettier": "^2.0.5",
		"size-plugin": "^2.0.1",
		"speed-measure-webpack-plugin": "^1.3.3",
		"standard-version": "^8.0.1",
		"typescript": "^3.9.6",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
