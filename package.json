{
	"name": "stack",
	"version": "0.0.1",
	"description": "a modern-day-tech stack boilerplate for React + TypeScript",
	"repository": "git@github.com:sreetamdas/stack.git",
	"author": "Sreetam Das <sreetamdas@gmail.com>",
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --config webpack.dev.js",
		"dev": "yarn start",
		"build": "webpack --config webpack.prod.js",
		"compile": "tsc --watch",
		"lint": "eslint 'src/*/**/*.{js,ts,tsx}' --cache --fix",
		"prettier": "prettier --write 'src/*/**/*.{ts,tsx}'",
		"typecheck": "tsc",
		"lint:check": "eslint 'src/*/**/*.{js,ts,tsx}'",
		"prettier:check": "prettier --check 'src/*/**/*.{ts,tsx}'",
		"release": "standard-version"
	},
	"dependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-is": "^16.13.1",
		"styled-components": "^5.1.1"
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
