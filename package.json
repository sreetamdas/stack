		"@types/prettier": "^2.0.2",
		"@types/react": "^16.9.43",
		"@types/react-dom": "^16.9.8",
		"@types/react-is": "^16.7.1",
		"@types/standard-version": "^7.0.0",
		"@types/styled-components": "^5.1.1",
		"@types/webpack": "^4.41.21",
		"@types/webpack-dev-server": "^3.11.0",
